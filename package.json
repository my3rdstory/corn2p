    "graphql": "16.8.1",
    "graphql-tag": "2.12.6",
    "if-logger": "0.12.5",
    "mingutils": "0.8.0",
    "node-telegram-bot-api": "0.66.0",
    "ramda": "0.30.1",
    "uuid": "10.0.0",
    "ws": "8.17.1"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.2",
    "@types/node-telegram-bot-api": "0.64.6",
    "@types/supertest": "6.0.2",
    "@types/ws": "8.5.10",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "supertest": "7.0.0",
